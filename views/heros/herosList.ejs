<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <header>
      <% include ../partials/header %>
    </header>

    <main>
      <h1>HEROS</h1>
      <ul>
        <% for (let i = 0; i < herosData.length; i++) { %>
          <li>
            <p><%= herosData[i].name %> hero id: <%= herosData[i].id %> nicknames: <%= herosData[i].nicknames %> talent: <%= herosData[i].talent %> contact info: <%= herosData[i].contact %></p>
            <p>age: <%= herosData[i].age %> price: <%= herosData[i].price %> rating: <%= herosData[i].rating %> level: <%= herosData[i].level %></p>
          </li>
        <% } %>
      </ul>
    </main>

  <footer>
    <% include ../partials/footer %>
  </footer>

  <% include ../partials/scripts %>
  </body>
</html>
